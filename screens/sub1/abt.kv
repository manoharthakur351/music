<subscreentwo>
	name: 'sub2_a'
	NavigationLayout:
		ScreenManager:
			Screen:
				BoxLayout:
					orientation: 'vertical'
					MDToolbar:
						title:'About'
						left_action_items: [["arrow-left",lambda x:  root.ch_sc()]]
					BoxLayout:
						orientation: 'vertical'
						ScrollView:
							GridLayout:
								id:g
								halign:'left'
								padding: 25
								spacing: 100
								cols:1
								height: self.minimum_height
								size_hint_y: None
								FloatLayout:
									id:f
									orientation:'vertical'
									size_hint_y: None
									size: root.width,root.height/3
									MDCard:
										elevation:7
										pos_hint: {'center_x': .5, 'center_y': .5}				
										size_hint: None,None
										size: root.width-50,(root.height/3)+70
										SmartTile:
											box_color:0,0,0,0
											on_release:
												print("tile released")
											AsyncImage:
												pos_hint: {'center_x': .5, 'center_y': .5}
												user_font_size: "128sp"
												size: 500,500
												source:'https://docs.google.com/drawings/d/e/2PACX-1vSzVq_NGXDLlYDqlxM0QxRSzhH-XSJC43zYbTRMMrygxxEz_Lti2kKo9XiFHGgYFo-jMYZYV9CAZnQP/pub?w=960&amp;h=720'
	
								MDLabel:
									markup: True
									size_hint:1, None
									size: self.texture_size
									text:app.textit("txt_mat/abt.txt")